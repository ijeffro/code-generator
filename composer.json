{
    "name": "ijeffro/codes",
    "description": "Acme Ltd - Code Generator",
    "keywords": [
        "ijeffro",
        "code-generator"
    ],
    "homepage": "https://github.com/ijeffro/codes",
    "license": "MIT",
    "version": "1.0.0" ,
    "type": "library",
    "authors": [
        {
            "name": "Phil Graham",
            "email": "ijeffrouk@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "spatie/laravel-data": "^1.0|^2.0|^3.0|^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ijeffro\\Codes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ijeffro\\Codes\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ijeffro\\Codes\\Providers\\CodeServiceProvider"
            ],
            "aliases": {
                "Generator": "Ijeffro\\Codes\\Facades\\GeneratorFacade"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
